+
+
+        try {
+            String display = new String(data, "UTF-8");
+            Log.d(TAG, "write:----> "+String.valueOf(display));
+        } catch (UnsupportedEncodingException e) {
+            throw new RuntimeException(e);
+        }
+
+    }

+
+import android.content.Context;
+import android.widget.TextView;
+import com.github.mikephil.charting.components.MarkerView;
+import com.github.mikephil.charting.utils.MPPointF;
+
+public class CustomMarkerView extends MarkerView {
+
+    private TextView markerTextView;
+
+    public CustomMarkerView(Context context, int layoutResource) {
+        super(context, layoutResource);
+
+        // Find and initialize your TextView or other UI elements inside the marker view
+        markerTextView = findViewById(R.id.tvContent);
+    }
+
+    @Override
+    public void refreshContent(Entry entry, Highlight highlight) {
+        // Customize the content of the marker view based on the selected data point
+        markerTextView.setText("Value: " + entry.getY());
+
+        super.refreshContent(entry, highlight);
+    }
+
+    @Override
+    public MPPointF getOffset() {
+        // Adjust the offset of the marker view based on your UI layout
+        return new MPPointF(-(getWidth() / 2), -getHeight());
+    }
+}

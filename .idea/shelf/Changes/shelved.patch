+            // Handle the case when the activity is not valid or already finishing
+            return;
+        }
+
+        try {
+            bsDialog = new BottomSheetDialog(activity, R.style.TransparentDialog);
+            bsDialog.setContentView(R.layout.layout_whos_reading);

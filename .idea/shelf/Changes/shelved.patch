+package com.humorstech.respyr.trends;
+
+import android.graphics.Color;
+
+import com.github.mikephil.charting.data.Entry;
+import com.github.mikephil.charting.formatter.ValueFormatter;
+
+public class CustomValueFormatter extends ValueFormatter {
+    public int getColor(Entry entry) {
+        // Set different colors based on the value of the entry
+        float value = entry.getY();
+        if (value > 80) {
+            return Color.RED; // Red for values above 80
+        } else if (value > 60) {
+            return Color.YELLOW; // Yellow for values between 60 and 80
+        } else {
+            return Color.GREEN; // Green for values below 60
+        }
+    }

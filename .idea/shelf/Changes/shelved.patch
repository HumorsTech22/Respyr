+
+        public MyHandler(BeforeReading activity) {
+            mActivity = new WeakReference<>(activity);
+        }
+
+        @Override
+        public void handleMessage(Message msg) {
+            if (msg.what == UsbService.MESSAGE_FROM_SERIAL_PORT) {
+                String data = (String) msg.obj;
+            }
+        }
+
+    }

+    private static int getReminder(int n, int[] factors) {
+        int reminder = n;
+
+        if (n <= 0) {
+            throw new IllegalArgumentException("n must be positive integer");
+        }
+
+        for (int i = 0; i < factors.length && reminder != 1; i++) {
+            int factor = factors[i];
+            while ((reminder % factor) == 0) {
+                reminder /= factor;
+            }
+        }
+        return reminder;
+    }

+            double out = s.m_b0 * w + s.m_b1 * m_v1 + s.m_b2 * m_v2;
+
+            m_v2 = m_v1;
+            m_v1 = w;
+
+            return out;
+        } else {
+            return in;
+        }
+    }
+
+    double m_v1; // v[-1]
+    double m_v2; // v[-2]
+}

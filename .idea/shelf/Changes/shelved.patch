+            }
+        }
+    }
+    private void drawRoundedBackground(Canvas canvas, float left, float top, float right, float bottom) {
+        float[] radii = {cornerRadius, cornerRadius, cornerRadius, cornerRadius, 0f, 0f, 0f, 0f};
+        Path backgroundPath = new Path();
